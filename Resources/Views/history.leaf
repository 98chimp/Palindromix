#extend("base")

#export("body") {

  <div class="row">
    <div class="u-full-width">
      <h4><center>List of Previously Searched Phrases</center></h4>
    </div>
  </div>

  #loop(phrases, "phrase") {
    <div class="row">
      <div class="six columns">
        <h5 align="right">#(phrase.content)</h5>
      </div>
      <div class="five columns">
          #if(phrase.ispalindromic) {
            <h3><span>ğŸ</span></h3>
          } ##else() {
            <h3><span>ğŸ</span></h3>
          }
      </div>
      <div class="one column">
      </div>
    </div>
  }
}
